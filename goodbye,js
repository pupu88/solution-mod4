(function (goodbye) {
    var usuarioDespedidor = {};
    var names = ["Julio","Juan","Jose","Julieta"];
    console.log(names);
    var despedida = "Adios";
    for(var i=0; i<names.length; i++){
        usuarioDespedidor.sayAdios = function () {
            console.log(despedida + usuarioDespedidor.name[i]);
        }
    }

    goodbye.usuarioDespedidor = usuarioDespedidor;
})(goodbye);
